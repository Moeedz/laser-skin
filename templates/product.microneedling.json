    "ss_steps_11_7cMyRT": {
      "type": "ss-steps-11",
      "blocks": {
        "step_jmRN9F": {
          "type": "step",
          "settings": {
            "step": "STEP 1",
            "title": "CONSULTATION",
            "text": "<p>Your consultation will be done on the day of your treatment, an extra consultation session is not needed.<br/><br/>We will begin with a detailed consultation by one of our qualified professionals.<br/><br/>We evaluate your skin type, texture, and areas of concern to decide whether this treatment is suitable for you<br/><br/>Your personalised plan is designed to deliver maximum improvement safely and effectively.</p>"
          }
        },
        "step_jiiz8x": {
          "type": "step",
          "settings": {
            "step": "STEP 2",
            "title": "PROCEDURE",
            "text": "<p>After a detailed consultation with one of our skin specialists, ultra-fine, gold-plated needles create micro-channels in the skin while controlled RF energy heats the deeper layers, triggering collagen production and instant tightening.</p>"
          }
        },
        "step_6jaGgE": {
          "type": "step",
          "settings": {
            "step": "STEP 3",
            "title": "RECOVERY",
            "text": "<p>You may experience mild redness or warmth after treatment, but this quickly settles, revealing firmer, rejuvenated skin that continues improving for weeks.</p>"
          }
        }
      },
      "block_order": [
        "step_jmRN9F",
        "step_jiiz8x",
        "step_6jaGgE"
      ],
