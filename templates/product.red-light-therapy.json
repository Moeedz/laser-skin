    "ss_steps_11_7cMyRT": {
      "type": "ss-steps-11",
      "blocks": {
        "step_jmRN9F": {
          "type": "step",
          "settings": {
            "step": "STEP 1",
            "title": "Consultation and Skin Preparation",
            "text": "<p>Your treatment begins with a short consultation to assess your skin condition, goals, and any recent procedures.<br/><br/>The skin is gently cleansed to remove makeup, sunscreen, or oils to ensure maximum light absorption.<br/><br/>This preparation ensures your session is safe, effective, and tailored to your skin’s needs.</p>"
          }
        },
        "step_jiiz8x": {
          "type": "step",
          "settings": {
            "step": "STEP 2",
            "title": " Celluma LED Therapy Session",
            "text": "<p>You will relax comfortably as the Celluma device is positioned above the treatment area.<br/><br/>The panel emits targeted red and near-infrared light wavelengths that penetrate deep into the skin to energise cells and trigger collagen renewal.<br/><br/>The session lasts around 20 to 30 minutes and is completely painless. Most clients describe it as warm, calming, and deeply relaxing.</p>"
          }
        },
        "step_6jaGgE": {
          "type": "step",
          "settings": {
            "step": "STEP 3",
            "title": "Recovery and Maintenance",
            "text": "<p>After your session, your skin will appear instantly refreshed with a healthy, natural glow.<br/><br/>There is no downtime, and you can return to normal activities straight away.</p>"
          }
        }
      },
      "block_order": [
        "step_jmRN9F",
        "step_jiiz8x",
        "step_6jaGgE"
      ],
