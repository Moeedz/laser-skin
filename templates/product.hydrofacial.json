n -->\n          {% if phone_number != blank %}\n            <div class=\"standalone-consultation-option\" onclick=\"openStandalonePhoneHoursModal(); event.preventDefault();\">\n              <div class=\"standalone-consultation-icon\">\n                <svg xmlns=\"http://www.w3.org/2000/svg\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\">\n                  <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M3 5a2 2 0 012-2h3.28a1 1 0 01.948.684l1.498 4.493a1 1 0 01-.502 1.21l-2.257 1.13a11.042 11.042 0 005.516 5.516l1.13-2.257a1 1 0 011.21-.502l4.493 1.498a1 1 0 01.684.949V19a2 2 0 01-2 2h-1C9.716 21 3 14.284 3 6V5z\" />\n                </svg>\n              </div>\n              <div class=\"standalone-consultation-details\">\n                <div class=\"standalone-consultation-method\">{{ phone_option_title }}</div>\n                <div class=\"standalone-consultation-info\">{{ phone_number }}</div>\n              </div>\n              <div class=\"standalone-consultation-arrow\">\n                <svg xmlns=\"http://www.w3.org/2000/svg\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\">\n                  <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M9 5l7 7-7 7\" />\n                </svg>\n              </div>\n            </div>\n          {% endif %}\n\n          <!-- WhatsApp Option -->\n          {% if whatsapp_number != blank %}\n            <a href=\"https://wa.me/{{ whatsapp_number | replace: ' ', '' | replace: '-', '' | replace: '+', '' }}\" target=\"_blank\" class=\"standalone-consultation-option\">\n              <div class=\"standalone-consultation-icon\">\n                <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"currentColor\">\n                  <path d=\"M17.472 14.382c-.297-.149-1.758-.867-2.03-.967-.273-.099-.471-.149-.67.149-.197.297-.767.966-.94 1.164-.173.199-.347.223-.644.074-.297-.149-1.255-.462-2.39-1.475-.883-.788-1