    "ss_steps_11_7cMyRT": {
      "type": "ss-steps-11",
      "blocks": {
        "step_jmRN9F": {
          "type": "step",
          "settings": {
            "step": "STEP 1",
            "title": "Consultation and Skin Assessment",
            "text": "<p>Your treatment begins with a consultation and skin assessment by one of our medical practitioners.<br/><br/>We evaluate your blemish or vein to confirm it is suitable for removal and select the most appropriate method, such as laser or plasma technology.<br/><br/>This ensures complete safety and precision throughout the process.</p>"
          }
        },
        "step_jiiz8x": {
          "type": "step",
          "settings": {
            "step": "STEP 2",
            "title": "Blemish Removal Treatment",
            "text": "<p>Once the area is cleansed and prepared, the blemish is carefully treated using laser or plasma energy to break down the targeted tissue without damaging the surrounding skin.<br/><br/>A topical numbing cream may be applied for comfort.</p>"
          }
        },
        "step_6jaGgE": {
          "type": "step",
          "settings": {
            "step": "STEP 3",
            "title": "Healing and Recovery",
            "text": "<p>After treatment, a small scab or redness may appear which heals naturally over several days.<br/><br/>The skin should be kept clean and protected from direct sunlight.<br/><br/>Once healed, the area appears smoother and clearer with minimal risk of scarring.</p>"
          }
        }
      },
      "block_order": [
        "step_jmRN9F",
        "step_jiiz8x",
        "step_6jaGgE"
      ],
