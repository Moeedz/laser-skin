    "ss_steps_11_7cMyRT": {
      "type": "ss-steps-11",
      "blocks": {
        "step_jmRN9F": {
          "type": "step",
          "settings": {
            "step": "STEP 1",
            "title": "PRE TREATMENT",
            "text": "<p>Your treatment begins with a consultation with one of our qualified medical professionals.<br/><br/>We assess your skin’s condition, hydration levels, and overall tone to determine how NCTF can best improve your skin health and appearance.</p>"
          }
        },
        "step_jiiz8x": {
          "type": "step",
          "settings": {
            "step": "STEP 2",
            "title": "PROCEDURE",
            "text": "<p>The NCTF solution is gently injected into the superficial layers of the skin using fine, precise microinjections.<br/><br/>This replenishes nutrients, enhances hydration, and encourages new collagen formation.<br/><br/>The treatment takes around 30 minutes and involves minimal discomfort, with most clients describing it as quick and easily tolerable.</p>"
          }
        },
        "step_6jaGgE": {
          "type": "step",
          "settings": {
            "step": "STEP 3",
            "title": "POST TREATMENT",
            "text": "<p>After treatment, the skin immediately appears more hydrated and luminous.<br/><br/>Over the following weeks, collagen production increases, improving firmness and texture.</p>"
          }
        }
      },
      "block_order": [
        "step_jmRN9F",
        "step_jiiz8x",
        "step_6jaGgE"
      ],
