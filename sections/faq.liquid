{%- if section.blocks.size > 0 -%}
  {%- assign color_scheme_hash = section.settings.color_scheme.settings.background_gradient | default: section.settings.color_scheme.settings.background | md5 -%}

  <div class="section-spacing color-scheme color-scheme--{{ section.settings.color_scheme.id }} color-scheme--bg-{{ color_scheme_hash }} {% if section.settings.separate_section_with_border %}bordered-section{% endif %}">
    <div class="container container--md">
      <div class="section-stack">
        {%- render 'section-header', subheading: section.settings.subheading, heading: section.settings.title, content: section.settings.content -%}

        <div class="faq">
          {%- assign categories_blocks = section.blocks | where: 'type', 'category' -%}

          {%- if section.settings.show_categories and categories_blocks.size > 0 -%}
            <faq-toc class="faq__toc hidden md:grid">
              {%- for block in categories_blocks -%}
                <a class="faq__toc-item {% if forloop.first %}is-active{% endif %}" href="#block-{{ block.id }}">{{ block.settings.title }}</a>
              {%- endfor -%}
            </faq-toc>
          {%- endif -%}

          <div class="faq__content">
            {%- for block in section.blocks -%}
              {%- case block.type -%}
                {%- when 'category' -%}
                  {%- if block.settings.title != blank -%}
                    <p id="block-{{ block.id }}" class="faq__category h4" {{ block.shopify_attributes }}>
                    {%- if block.settings.custom_icon != blank -%}
                      {%- capture sizes -%}{{ block.settings.icon_width }}px{%- endcapture -%}
                      {%- capture widths -%}{{ block.settings.icon_width }}, {{ block.settings.icon_width | times: 2 | at_most: block.settings.custom_icon.width }}{%- endcapture -%}
                      {%- capture style -%}--image-max-width: {{ block.settings.icon_width }}px{%- endcapture -%}

                      {{- block.settings.custom_icon | image_url: width: block.settings.custom_icon.width | image_tag: sizes: sizes, widths: widths, class: 'constrained-image', style: style -}}
                    {%- elsif block.settings.icon != 'none' -%}
                      {%- render 'icon' with block.settings.icon, width: block.settings.icon_width -%}
                    {%- endif -%}

                      {{- block.settings.title -}}
                    </p>
                  {%- endif -%}

                {%- when 'question' -%}
                  {%- if block.settings.question != blank and block.settings.answer != blank -%}
                    {%- capture answer -%}
                      <div class="prose">
                        {{- block.settings.answer -}}
                      </div>
                      
                      {%- comment -%}Embedded Steps Section - Inline{%- endcomment -%}
                      {%- if block.settings.show_embedded_section -%}
                        {%- assign block_id = block.id -%}
                        {%- assign image = block.settings.embedded_image -%}
                        {%- assign layout = block.settings.embedded_layout -%}
                        {%- assign layout_mobile = block.settings.embedded_layout_mobile -%}
                        {%- assign layout_gap = block.settings.embedded_layout_gap -%}
                        {%- assign layout_gap_mobile = block.settings.embedded_layout_gap_mobile -%}
                        {%- assign image_ratio = block.settings.embedded_image_ratio -%}
                        {%- assign image_ratio_mobile = block.settings.embedded_image_ratio_mobile -%}

                        <style>
                          .faq-steps-{{ block_id }} {
                            display: grid;
                            align-items: center;
                            grid-template-columns: 1fr;
                            gap: {{ layout_gap_mobile }}px;
                            padding: 20px 0;
                            margin-top: 2rem;
                          }

                          .faq-steps-content-{{ block_id }} {
                            display: flex;
                            flex-direction: column;
                            justify-content: center;
                            gap: 30px;
                            position: relative;
                          }

                          .faq-steps-image-{{ block_id }} {
                            border-radius: 8px;
                            overflow: hidden;
                          }

                          .faq-steps-image-{{ block_id }} img,
                          .faq-steps-image-{{ block_id }} svg {
                            display: block;
                            width: 100%;
                            height: 100%;
                            object-fit: cover;
                          }

                          .faq-steps-image-{{ block_id }} svg {
                            background-color: #AFAFAF;
                          }

                          .faq-steps-item-{{ block_id }} {
                            display: flex;
                            align-items: center;
                            gap: 20px;
                          }

                          .faq-steps-step-{{ block_id }} {
                            min-width: 60px;
                            text-align: center;
                            position: relative;
                            z-index: 2;
                            padding: 8px 12px;
                            border: 1px solid #121212;
                            border-radius: 100px;
                            background-color: #FFFFFF;
                            margin: 0;
                            font-size: 12px;
                            color: #121212;
                            line-height: 1;
                            font-weight: 600;
                          }

                          .faq-steps-item-content-{{ block_id }} {
                            flex: 1 1 0;
                          }

                          .faq-steps-title-{{ block_id }} {
                            margin: 0 0 8px 0;
                            font-size: 18px;
                            color: #121212;
                            line-height: 1.3;
                            font-weight: 700;
                          }

                          .faq-steps-text-{{ block_id }} {
                            margin: 0;
                          }

                          .faq-steps-text-{{ block_id }} * {
                            margin: 0;
                            font-size: 14px;
                            color: #121212;
                            line-height: 1.5;
                          }

                          .faq-steps-border-{{ block_id }} {
                            left: 30px;
                            display: block;
                            position: absolute;
                            background-image: repeating-linear-gradient(#121212, #121212 6px, transparent 6px, transparent calc(9px));
                            background-size: 1px 100%;
                            background-repeat: no-repeat;
                            width: 1px;
                            height: 100%;
                          }

                          @media(min-width: 1024px) {
                            .faq-steps-{{ block_id }} {
                              grid-template-columns: 1fr 1fr;
                              gap: {{ layout_gap }}px;
                            }

                            .faq-steps-content-{{ block_id }} {
                              gap: 40px;
                            }

                            .faq-steps-step-{{ block_id }} {
                              padding: 10px 16px;
                              font-size: 14px;
                              min-width: 70px;
                            }

                            .faq-steps-title-{{ block_id }} {
                              font-size: 20px;
                            }

                            .faq-steps-text-{{ block_id }} * {
                              font-size: 15px;
                            }

                            .faq-steps-border-{{ block_id }} {
                              left: 35px;
                            }
                          }

                          {% if image_ratio_mobile == 'portrait' %}
                            .faq-steps-image-{{ block_id }} {
                              aspect-ratio: 9.6/13;
                            }
                          {% elsif image_ratio_mobile == 'landscape' %}
                            .faq-steps-image-{{ block_id }} {
                              aspect-ratio: 12/8;
                            }
                          {% elsif image_ratio_mobile == 'square' %}
                            .faq-steps-image-{{ block_id }} {
                              aspect-ratio: 1/1;
                            }
                          {% else %}
                            .faq-steps-image-{{ block_id }} {
                              aspect-ratio: auto;
                            }
                          {% endif %}

                          {% if image_ratio == 'portrait' %}
                            @media(min-width: 1024px) {
                              .faq-steps-image-{{ block_id }} {
                                aspect-ratio: 9.6/13;
                              }
                            }
                          {% elsif image_ratio == 'landscape' %}
                            @media(min-width: 1024px) {
                              .faq-steps-image-{{ block_id }} {
                                aspect-ratio: 12/8;
                              }
                            }
                          {% elsif image_ratio == 'square' %}
                            @media(min-width: 1024px) {
                              .faq-steps-image-{{ block_id }} {
                                aspect-ratio: 1/1;
                              }
                            }
                          {% else %}
                            @media(min-width: 1024px) {
                              .faq-steps-image-{{ block_id }} {
                                aspect-ratio: auto;
                              }
                            }
                          {% endif %}

                          {% if layout_mobile == 'image_content' %}
                            .faq-steps-image-{{ block_id }} {
                              order: 1;
                            }
                            .faq-steps-content-{{ block_id }} {
                              order: 2;
                            }
                          {% else %}
                            .faq-steps-image-{{ block_id }} {
                              order: 2;
                            }
                            .faq-steps-content-{{ block_id }} {
                              order: 1;
                            }
                          {% endif %}

                          {% if layout == 'image_content' %}
                            @media (min-width: 1024px) {
                              .faq-steps-image-{{ block_id }} {
                                order: 1;
                              }
                              .faq-steps-content-{{ block_id }} {
                                order: 2;
                              }
                            }
                          {% else %}
                            @media (min-width: 1024px) {
                              .faq-steps-image-{{ block_id }} {
                                order: 2;
                              }
                              .faq-steps-content-{{ block_id }} {
                                order: 1;
                              }
                            }
                          {% endif %}
                        </style>

                        <div class="faq-steps-{{ block_id }}">
                          <div class="faq-steps-image-{{ block_id }}">
                            {% if image != blank %}
                              {{
                                image
                                | image_url: width: 1200
                                | image_tag:
                                  loading: 'lazy',
                                  width: image.width,
                                  height: image.height,
                                  sizes: '(min-width: 750px) calc((100vw - 130px) / 2), calc(100vw - 50px)'
                              }}
                            {% else %}
                              {{ 'image' | placeholder_svg_tag }}
                            {% endif %}
                          </div>
                          <div class="faq-steps-content-{{ block_id }}">
                            <div class="faq-steps-border-{{ block_id }}"></div>
                            
                            {%- for i in (1..4) -%}
                              {%- assign step_num = 'embedded_step_' | append: i -%}
                              {%- assign step_title = 'embedded_step_' | append: i | append: '_title' -%}
                              {%- assign step_text = 'embedded_step_' | append: i | append: '_text' -%}
                              
                              {%- if block.settings[step_num] != blank or block.settings[step_title] != blank -%}
                                <div class="faq-steps-item-{{ block_id }}">
                                  {% if block.settings[step_num] != blank %}
                                    <div class="faq-steps-step-{{ block_id }}">{{ block.settings[step_num] }}</div>
                                  {% endif %}
                                  <div class="faq-steps-item-content-{{ block_id }}">
                                    {% if block.settings[step_title] != blank %}
                                      <h4 class="faq-steps-title-{{ block_id }}">{{ block.settings[step_title] }}</h4>
                                    {% endif %}
                                    {% if block.settings[step_text] != blank %}
                                      <div class="faq-steps-text-{{ block_id }}">{{ block.settings[step_text] }}</div>
                                    {% endif %}
                                  </div>
                                </div>
                              {%- endif -%}
                            {%- endfor -%}
                          </div>
                        </div>
                      {%- endif -%}
                    {%- endcapture -%}

                    {%- render 'accordion', title: block.settings.question, content: answer, show_title_as_text: true, block: block -%}
                  {%- endif -%}
              {%- endcase -%}
            {%- endfor -%}
          </div>
        </div>
      </div>
    </div>
  </div>

  {%- comment -%}We also output the content with JSON microdata for SEO{%- endcomment -%}

  <script type="application/ld+json">
    {%- assign question_blocks = section.blocks | where: 'type', 'question' -%}

    {
      "@context": "https://schema.org",
      "@type": "FAQPage",
      "mainEntity": [
        {%- for block in question_blocks -%}
          {
            "@type": "Question",
            "name": {{ block.settings.question | json }},
            "acceptedAnswer": {
              "@type": "Answer",
              "text": {{ block.settings.answer | json }}
            }
          }{% unless forloop.last %},{% endunless %}
        {%- endfor -%}
      ]
    }
  </script>
{%- endif -%}

