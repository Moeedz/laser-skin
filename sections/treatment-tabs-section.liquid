            <!-- CTA Cards -->
            <div class="cta-cards">
              <!-- Book Consultation Card - Now triggers modal -->
              <div class="cta-card consultation-card" onclick="openConsultationModal('{{ block.settings.tab_identifier }}')">
                <div class="cta-card-header">
                  <h3>{{ block.settings.consultation_heading }}</h3>
                  {% if block.settings.consultation_badge != blank %}
                    <span class="cta-badge">{{ block.settings.consultation_badge }}</span>
                  {% endif %}
                </div>
                <p>{{ block.settings.consultation_description }}</p>
              </div>

              <!-- Buy Package Card -->
              <div class="cta-card package-card-toggle" onclick="togglePackages('{{ block.settings.tab_identifier }}')">
                <div class="cta-card-header">
                  <h3>{{ block.settings.package_heading }}</h3>
                  <span class="cta-price" data-package-price="{{ block.settings.tab_identifier }}">
                    {% assign first_package = section.blocks | where: "type", "package" | where: "settings.parent_tab", block.settings.tab_identifier | first %}
                    {% if first_package %}
                      from {{ first_package.settings.price }}
                    {% endif %}
                  </span>
                </div>
                <p>{{ block.settings.package_description }}</p>
              </div>

              <!-- Tab Navigation - Desktop Only (Below CTA Cards) -->
              <div class="tab-navigation content-tabs desktop-tabs">
                {% for tab_block in section.blocks %}
                  {% if tab_block.type == 'tab' %}
                    <button 
                      class="tab-button {% if tab_block.settings.tab_identifier == block.settings.tab_identifier %}active{% endif %}" 
                      data-tab="{{ tab_block.settings.tab_identifier }}"
                      onclick="switchTab('{{ tab_block.settings.tab_identifier }}')">
                      {{ tab_block.settings.tab_name }}
                    </button>
                  {% endif %}
                {% endfor %}
              </div>
            </div>
          </div>
        </div>

        <!-- Consultation Modal for this tab -->
        <div class="consultation-modal-overlay" id="consultation-modal-{{ block.settings.tab_identifier }}">
          <div class="consultation-modal">
            <div class="modal-header">
              <button class="modal-close" onclick="closeConsultationModal('{{ block.settings.tab_identifier }}')">
                <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M6 18L18 6M6 6l12 12" />
                </svg>
              </button>
              <h3 class="modal-title">{{ block.settings.modal_title | default: 'How would you like to consult?' }}</h3>
              <p class="modal-subtitle">{{ block.settings.modal_subtitle | default: 'Choose your preferred method to get in touch with us' }}</p>
            </div>
            <div class="modal-content">
              <div class="consultation-options">
                
                <!-- Contact Page Option -->
                {% if block.settings.contact_page_url != blank %}
                  <a href="{{ block.settings.contact_page_url }}" class="consultation-option">
                    <div class="consultation-icon">
                      <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3 8l7.89 5.26a2 2 0 002.22 0L21 8M5 19h14a2 2 0 002-2V7a2 2 0 00-2-2H5a2 2 0 00-2 2v10a2 2 0 002 2z" />
                      </svg>
                    </div>
                    <div class="consultation-details">
                      <div class="consultation-method">{{ block.settings.contact_option_title | default: 'Contact Form' }}</div>
                      <div class="consultation-info">{{ block.settings.contact_option_description | default: 'Fill out our online form' }}</div>
                    </div>
                    <div class="consultation-arrow">
                      <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7" />
                      </svg>
                    </div>
                  </a>
                {% endif %}

                <!-- Phone Option - Now opens hours modal -->
                {% if block.settings.phone_number != blank %}
                  <div class="consultation-option" onclick="openPhoneHoursModal('{{ block.settings.tab_identifier }}'); event.preventDefault();">
                    <div class="consultation-icon">
                      <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3 5a2 2 0 012-2h3.28a1 1 0 01.948.684l1.498 4.493a1 1 0 01-.502 1.21l-2.257 1.13a11.042 11.042 0 005.516 5.516l1.13-2.257a1 1 0 011.21-.502l4.493 1.498a1 1 0 01.684.949V19a2 2 0 01-2 2h-1C9.716 21 3 14.284 3 6V5z" />
                      </svg>
                    </div>
                    <div class="consultation-details">
                      <div class="consultation-method">{{ block.settings.phone_option_title | default: 'Call Us' }}</div>
                      <div class="consultation-info">{{ block.settings.phone_number }}</div>
                    </div>
                    <div class="consultation-arrow">
                      <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7" />
                      </svg>
                    </div>
                  </div>
                {% endif %}

                <!-- WhatsApp Option -->
                {% if block.settings.whatsapp_number != blank %}
                  <a href="https://wa.me/{{ block.settings.whatsapp_number | replace: ' ', '' | replace: '-', '' | replace: '+', '' }}" target="_blank" class="consultation-option">
                    <div class="consultation-icon">
                      <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M8 12h.01M12 12h.01M16 12h.01M21 12c0 4.418-4.03 8-9 8a9.863 9.863 0 01-4.255-.949L3 20l1.395-3.72C3.512 15.042 3 13.574 3 12c0-4.418 4.03-8 9-8s9 3.582 9 8z" />
                      </svg>
                    </div>
                    <div class="consultation-details">
                      <div class="consultation-method">{{ block.settings.whatsapp_option_title | default: 'WhatsApp' }}</div>
                      <div class="consultation-info">{{ block.settings.whatsapp_display_number | default: block.settings.whatsapp_number }}</div>
                    </div>
                    <div class="consultation-arrow">
                      <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7" />
                      </svg>
                    </div>
                  </a>
                {% endif %}
              </div>
            </div>
          </div>
        </div>

        <!-- Phone Hours Modal for this tab -->
        <div class="consultation-modal-overlay" id="phone-hours-modal-{{ block.settings.tab_identifier }}">
          <div class="phone-hours-modal">
            <button class="modal-close" onclick="closePhoneHoursModal('{{ block.settings.tab_identifier }}')">
              <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M6 18L18 6M6 6l12 12" />
              </svg>
            </button>
            
            <div class="phone-icon-circle">
              <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3 5a2 2 0 012-2h3.28a1 1 0 01.948.684l1.498 4.493a1 1 0 01-.502 1.21l-2.257 1.13a11.042 11.042 0 005.516 5.516l1.13-2.257a1 1 0 011.21-.502l4.493 1.498a1 1 0 01.684.949V19a2 2 0 01-2 2h-1C9.716 21 3 14.284 3 6V5z" />
              </svg>
            </div>

            <h3 class="phone-modal-title">Call Us</h3>

            <div class="store-status" id="store-status-{{ block.settings.tab_identifier }}">
              <span class="status-icon">âœ“</span>
              <span class="status-text">We are currently open</span>
            </div>

            <div class="closed-message" id="closed-message-{{ block.settings.tab_identifier }}" style="display: none;">
              <p>{{ block.settings.closed_message | default: 'You can send us a WhatsApp, send us an email or call back during opening hours' }}</p>
            </div>

            <div class="opening-hours">
              <h4 class="hours-heading">Opening Hours</h4>
              <div class="hours-list">
                <div class="hours-row">
                  <span class="day">Monday</span>
                  <span class="time">{{ block.settings.monday_hours | default: 'Closed' }}</span>
                </div>
                <div class="hours-row">
                  <span class="day">Tuesday</span>
                  <span class="time">{{ block.settings.tuesday_hours | default: '10:00AM-6:00PM' }}</span>
                </div>
                <div class="hours-row">
                  <span class="day">Wednesday</span>
                  <span class="time">{{ block.settings.wednesday_hours | default: '10:00AM-6:00PM' }}</span>
                </div>
                <div class="hours-row">
                  <span class="day">Thursday</span>
                  <span class="time">{{ block.settings.thursday_hours | default: 'Closed' }}</span>
                </div>
                <div class="hours-row">
                  <span class="day">Friday</span>
                  <span class="time">{{ block.settings.friday_hours | default: '10:00AM-6:00PM' }}</span>
                </div>
                <div class="hours-row">
                  <span class="day">Saturday</span>
                  <span class="time">{{ block.settings.saturday_hours | default: '10:00AM-4:00PM' }}</span>
                </div>
                <div class="hours-row">
                  <span class="day">Sunday</span>
                  <span class="time">{{ block.settings.sunday_hours | default: 'Closed' }}</span>
                </div>
              </div>
            </div>

            <a href="tel:{{ block.settings.phone_number | replace: ' ', '' | replace: '-', '' }}" class="call-now-button" id="call-button-{{ block.settings.tab_identifier }}">
              Call Now
            </a>
          </div>
        </div>

        <!-- Packages Container - Right below desktop tabs -->
        <div class="packages-container" data-packages="{{ block.settings.tab_identifier }}">
          <div class="packages-grid">
            {% for package_block in section.blocks %}
              {% if package_block.type == 'package' and package_block.settings.parent_tab == block.settings.tab_identifier %}
                <div class="package-card">
                  <div class="package-card-header" onclick="toggleSessionOptions('{{ package_block.id }}')">
                    <h4>{{ package_block.settings.package_name }}</h4>
                    <div class="package-price">{{ package_block.settings.price }}</div>
                  </div>
                  
                  <!-- Session Options -->
                  <div class="session-options" id="session-{{ package_block.id }}">
                    <div class="session-selector-heading">Select Number of Session</div>
                    
                    {% if package_block.settings.show_session_1 %}
                      <div class="session-option" data-link="{{ package_block.settings.session_1_link }}">
                        <div class="session-info">
                          <span class="session-label">{{ package_block.settings.session_1_label }}</span>
                          <span class="session-price">{{ package_block.settings.session_1_price }}</span>
                        </div>
                        <div class="session-details">
                          <span class="session-total">{{ package_block.settings.session_1_total }}</span>
                          <span class="session-per">{{ package_block.settings.session_1_per_session }}</span>
                        </div>
                      </div>
                    {% endif %}

                    {% if package_block.settings.show_session_3 %}
                      <div class="session-option" data-link="{{ package_block.settings.session_3_link }}">
                        <div class="session-info">
                          <span class="session-label">{{ package_block.settings.session_3_label }}</span>
                          <span class="session-price">{{ package_block.settings.session_3_price }}</span>
                        </div>
                        <div class="session-details">
                          <span class="session-total">{{ package_block.settings.session_3_total }}</span>
                          <span class="session-per">{{ package_block.settings.session_3_per_session }}</span>
                        </div>
                      </div>
                    {% endif %}

                    {% if package_block.settings.show_session_6 %}
                      <div class="session-option" data-link="{{ package_block.settings.session_6_link }}">
                        <div class="session-info">
                          <span class="session-label">{{ package_block.settings.session_6_label }}</span>
                          <span class="session-price">{{ package_block.settings.session_6_price }}</span>
                        </div>
                        <div class="session-details">
                          <span class="session-total">{{ package_block.settings.session_6_total }}</span>
                          <span class="session-per">{{ package_block.settings.session_6_per_session }}</span>
                        </div>
                      </div>
                    {% endif %}

                    {% if package_block.settings.show_session_10 %}
                      <div class="session-option" data-link="{{ package_block.settings.session_10_link }}">
                        <div class="session-info">
                          <span class="session-label">{{ package_block.settings.session_10_label }}</span>
                          <span class="session-price">{{ package_block.settings.session_10_price }}</span>
                        </div>
                        <div class="session-details">
                          <span class="session-total">{{ package_block.settings.session_10_total }}</span>
                          <span class="session-per">{{ package_block.settings.session_10_per_session }}</span>
                        </div>
                      </div>
                    {% endif %}

                    <button class="package-select session-select">SELECT</button>
                  </div>
                </div>
              {% endif %}
            {% endfor %}
          </div>
        </div>

        <!-- Collapsible Tabs Section (Static - doesn't change with tabs) -->
        <div class="collapsible-tabs-section">
          {% if section.settings.show_collapsible_1 %}
            <div class="collapsible-tab" id="collapsible-1-{{ block.settings.tab_identifier }}">
              <div class="collapsible-header" onclick="toggleCollapsible('collapsible-1-{{ block.settings.tab_identifier }}')">
                <div class="collapsible-header-left">
                  <div class="collapsible-icon">
                    {{ section.settings.collapsible_1_icon }}
                  </div>
                  <div class="collapsible-title">{{ section.settings.collapsible_1_title }}</div>
                </div>
                <div class="collapsible-arrow">
                  <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 9l-7 7-7 7" />
                  </svg>
                </div>
              </div>
              <div class="collapsible-content">
                <div class="collapsible-content-inner">
                  {{ section.settings.collapsible_1_content }}
                </div>
              </div>
            </div>
          {% endif %}

          {% if section.settings.show_collapsible_2 %}
            <div class="collapsible-tab" id="collapsible-2-{{ block.settings.tab_identifier }}">
              <div class="collapsible-header" onclick="toggleCollapsible('collapsible-2-{{ block.settings.tab_identifier }}')">
                <div class="collapsible-header-left">
                  <div class="collapsible-icon">
                    {{ section.settings.collapsible_2_icon }}
                  </div>
                  <div class="collapsible-title">{{ section.settings.collapsible_2_title }}</div>
                </div>
                <div class="collapsible-arrow">
                  <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 9l-7 7-7 7" />
                  </svg>
                </div>
              </div>
              <div class="collapsible-content">
                <div class="collapsible-content-inner">
                  {{ section.settings.collapsible_2_content }}
                </div>
              </div>
            </div>
          {% endif %}

          {% if section.settings.show_collapsible_3 %}
            <div class="collapsible-tab" id="collapsible-3-{{ block.settings.tab_identifier }}">
              <div class="collapsible-header" onclick="toggleCollapsible('collapsible-3-{{ block.settings.tab_identifier }}')">
                <div class="collapsible-header-left">
                  <div class="collapsible-icon">
                    {{ section.settings.collapsible_3_icon }}
                  </div>
                  <div class="collapsible-title">{{ section.settings.collapsible_3_title }}</div>
                </div>
                <div class="collapsible-arrow">
                  <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 9l-7 7-7 7" />
                  </svg>
                </div>
              </div>
              <div class="collapsible-content">
                <div class="collapsible-content-inner">
                  {{ section.settings.collapsible_3_content }}
                </div>
              </div>
            </div>
          {% endif %}

          {% if section.settings.show_collapsible_4 %}
