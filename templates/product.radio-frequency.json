    "ss_steps_11_7cMyRT": {
      "type": "ss-steps-11",
      "blocks": {
        "step_jmRN9F": {
          "type": "step",
          "settings": {
            "step": "STEP 1",
            "title": "Consultation and Skin Assessment",
            "text": "<p>Your treatment begins with a consultation where your practitioner assesses your skin’s elasticity, concerns, and goals.</p><p>This allows us to determine the most effective radiofrequency settings for your treatment area and create a personalised plan.</p>"
          }
        },
        "step_jiiz8x": {
          "type": "step",
          "settings": {
            "step": "STEP 2",
            "title": "Radio Frequency Tightening Treatment",
            "text": "<p>A handheld device is gently applied to the skin, delivering controlled RF heat deep into the dermis. The treatment feels comfortable and relaxing, similar to a warm massage.</p><p>A session usually lasts 20 to 40 minutes depending on the area.</p>"
          }
        },
        "step_6jaGgE": {
          "type": "step",
          "settings": {
            "step": "STEP 3",
            "title": "Collagen Renewal and Maintenance",
            "text": "<p>After your session, the skin may feel warm and slightly flushed, which settles quickly.</p><p>Collagen production continues over the following weeks, gradually improving firmness and definition.</p>"
          }
        }
      },
      "block_order": [
        "step_jmRN9F",
        "step_jiiz8x",
        "step_6jaGgE"
      ],
