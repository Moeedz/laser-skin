    "ss_steps_11_7cMyRT": {
      "type": "ss-steps-11",
      "blocks": {
        "step_jmRN9F": {
          "type": "step",
          "settings": {
            "step": "STEP 1",
            "title": "Pre Treatment",
            "text": "<p>Your treatment begins with a detailed consultation with one of our qualified medical professionals.<br/><br/>We assess your skin’s texture, hydration, and overall condition to determine the most effective injection points and concentration for your goals.<br/><br/>This ensures your plan is precise, safe, and fully tailored to your skin’s needs.</p>"
          }
        },
        "step_jiiz8x": {
          "type": "step",
          "settings": {
            "step": "STEP 2",
            "title": "Polynucleotide Injection Treatment",
            "text": "<p>During treatment, small amounts of the polynucleotide solution are injected into the skin using fine micro-needles.<br/><br/>The formula begins working immediately to boost hydration and cellular repair while supporting collagen and elastin regeneration.<br/><br/>The procedure takes around 20 to 30 minutes, and a topical numbing cream can be applied to enhance comfort.</p>"
          }
        },
        "step_6jaGgE": {
          "type": "step",
          "settings": {
            "step": "STEP 3",
            "title": "Restoration and Maintenance",
            "text": "<p>Over the following days and weeks, the skin becomes firmer, smoother, and more luminous as cell turnover increases.</p>"
          }
        }
      },
      "block_order": [
        "step_jmRN9F",
        "step_jiiz8x",
        "step_6jaGgE"
      ],
