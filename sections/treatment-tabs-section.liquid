<script>
  function switchTab(tabId) {
    // Update ALL tab buttons (both top and content tabs)
    document.querySelectorAll('.tab-button').forEach(btn => {
      btn.classList.remove('active');
    });
    document.querySelectorAll(`[data-tab="${tabId}"]`).forEach(btn => {
      btn.classList.add('active');
    });

    // Update tab content
    document.querySelectorAll('.tab-content').forEach(content => {
      content.classList.remove('active');
    });
    document.querySelector(`[data-tab-content="${tabId}"]`).classList.add('active');

    // Close any open package containers
    document.querySelectorAll('.packages-container').forEach(container => {
      container.classList.remove('active');
    });

    // Close any open session options
    document.querySelectorAll('.session-options').forEach(session => {
      session.classList.remove('active');
    });
  }

  function togglePackages(tabId) {
    const packagesContainer = document.querySelector(`[data-packages="${tabId}"]`);
    packagesContainer.classList.toggle('active');
  }

  function toggleSessionOptions(packageId) {
    const sessionOptions = document.getElementById(`session-${packageId}`);
    sessionOptions.classList.toggle('active');
  }

  function toggleCollapsible(collapsibleId) {
    const collapsibleTab = document.getElementById(collapsibleId);
    collapsibleTab.classList.toggle('active');
  }

  // Modal Functions
  function openConsultationModal(tabId) {
    const modal = document.getElementById(`consultation-modal-${tabId}`);
    if (modal) {
      modal.classList.add('active');
      document.body.style.overflow = 'hidden'; // Prevent body scroll when modal is open
    }
  }

  function closeConsultationModal(tabId) {
    const modal = document.getElementById(`consultation-modal-${tabId}`);
    if (modal) {
      modal.classList.remove('active');
      document.body.style.overflow = ''; // Restore body scroll
    }
  }

  // Phone Hours Modal Functions
  function openPhoneHoursModal(tabId) {
    // First close the consultation modal
    closeConsultationModal(tabId);
    
    // Open phone hours modal
    const modal = document.getElementById(`phone-hours-modal-${tabId}`);
    if (modal) {
      modal.classList.add('active');
      document.body.style.overflow = 'hidden';
      
      // Check if store is open and update status
      checkStoreStatus(tabId);
      
      // Highlight current day
      highlightCurrentDay(tabId);
    }
  }

  function closePhoneHoursModal(tabId) {
    const modal = document.getElementById(`phone-hours-modal-${tabId}`);
    if (modal) {
      modal.classList.remove('active');
      document.body.style.overflow = '';
    }
  }

  function checkStoreStatus(tabId) {
    const statusElement = document.getElementById(`store-status-${tabId}`);
    if (!statusElement) return;

    // Get current time
    const now = new Date();
    const currentDay = now.getDay(); // 0 = Sunday, 1 = Monday, etc.
    const currentHour = now.getHours();
    const currentMinute = now.getMinutes();
    const currentTime = currentHour * 60 + currentMinute; // Convert to minutes for easier comparison

    // Get store hours for current day from the modal
    const modal = document.getElementById(`phone-hours-modal-${tabId}`);
    const hoursRows = modal.querySelectorAll('.hours-row');
    const todayRow = hoursRows[currentDay === 0 ? 6 : currentDay - 1]; // Adjust for Sunday being 0
    const hoursText = todayRow.querySelector('.time').textContent.trim();

    // Check if store is open
    let isOpen = false;
    if (hoursText.toLowerCase() !== 'closed') {
      // Parse hours (format: "10:00AM-6:00PM")
      const matches = hoursText.match(/(\d{1,2}):(\d{2})(AM|PM)-(\d{1,2}):(\d{2})(AM|PM)/i);
      if (matches) {
        let openHour = parseInt(matches[1]);
        const openMinute = parseInt(matches[2]);
        const openPeriod = matches[3].toUpperCase();
        let closeHour = parseInt(matches[4]);
        const closeMinute = parseInt(matches[5]);
        const closePeriod = matches[6].toUpperCase();

        // Convert to 24-hour format
        if (openPeriod === 'PM' && openHour !== 12) openHour += 12;
        if (openPeriod === 'AM' && openHour === 12) openHour = 0;
        if (closePeriod === 'PM' && closeHour !== 12) closeHour += 12;
        if (closePeriod === 'AM' && closeHour === 12) closeHour = 0;

        const openTime = openHour * 60 + openMinute;
        const closeTime = closeHour * 60 + closeMinute;

        isOpen = currentTime >= openTime && currentTime < closeTime;
      }
    }

    // Update status display
    const statusIcon = statusElement.querySelector('.status-icon');
    const statusText = statusElement.querySelector('.status-text');
    const closedMessage = document.getElementById(`closed-message-${tabId}`);
    const callButton = document.getElementById(`call-button-${tabId}`);
    
    if (isOpen) {
      statusElement.classList.remove('closed');
      statusIcon.textContent = 'âœ“';
      statusText.textContent = 'We are currently open';
      if (closedMessage) {
        closedMessage.style.display = 'none';
      }
      if (callButton) {
        callButton.textContent = 'Call Now';
        callButton.classList.remove('disabled');
        callButton.style.pointerEvents = 'auto';
        callButton.style.background = '#007AFF';
      }
    } else {
      statusElement.classList.add('closed');
      statusIcon.textContent = 'âœ—';
      statusText.textContent = 'We are currently closed';
      if (closedMessage) {
        closedMessage.style.display = 'block';
      }
      if (callButton) {
        callButton.textContent = 'Currently Closed';
        callButton.classList.add('disabled');
        callButton.style.pointerEvents = 'none';
        callButton.style.background = '#cccccc';
      }
    }
  }

  function highlightCurrentDay(tabId) {
    const modal = document.getElementById(`phone-hours-modal-${tabId}`);
    if (!modal) return;

    const now = new Date();
    const currentDay = now.getDay(); // 0 = Sunday, 1 = Monday, etc.
    
    const hoursRows = modal.querySelectorAll('.hours-row');
    hoursRows.forEach(row => row.classList.remove('current-day'));
    
    // Highlight the current day row
    const todayRow = hoursRows[currentDay === 0 ? 6 : currentDay - 1]; // Adjust for Sunday being 0
    if (todayRow) {
      todayRow.classList.add('current-day');
    }
  }

  // Close modal when clicking on overlay
  document.addEventListener('DOMContentLoaded', function() {
    // Handle session selection
    let selectedSessionLink = '';

    document.querySelectorAll('.session-option').forEach(option => {
      option.addEventListener('click', function() {
        // Remove selected class from siblings
        const parent = this.parentElement;
        parent.querySelectorAll('.session-option').forEach(opt => {
          opt.classList.remove('selected');
        });
        
        // Add selected class to clicked option
        this.classList.add('selected');
        
        // Store the link from data attribute
        selectedSessionLink = this.getAttribute('data-link');
        
        // Enable the SELECT button
        const selectButton = parent.querySelector('.session-select');
        selectButton.classList.add('enabled');
        
        // Add click handler to SELECT button
        selectButton.onclick = function() {
          if (selectedSessionLink && selectedSessionLink !== '') {
            window.location.href = selectedSessionLink;
          }
        };
      });
    });

    // Close modal when clicking on overlay (not on modal content)
    document.querySelectorAll('.consultation-modal-overlay').forEach(overlay => {
      overlay.addEventListener('click', function(e) {
        if (e.target === this) {
          this.classList.remove('active');
          document.body.style.overflow = '';
        }
      });
    });

    // Close modal with Escape key
    document.addEventListener('keydown', function(e) {
      if (e.key === 'Escape') {
        document.querySelectorAll('.consultation-modal-overlay.active').forEach(modal => {
          modal.classList.remove('active');
          document.body.style.overflow = '';
        });
      }
    });
  });
</script>

