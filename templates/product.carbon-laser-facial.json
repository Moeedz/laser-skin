 viewBox=\"0 0 24 24\" stroke=\"currentColor\">\n          <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M6 18L18 6M6 6l12 12\" />\n        </svg>\n      </button>\n      \n      <div class=\"standalone-phone-icon-circle\">\n        <svg xmlns=\"http://www.w3.org/2000/svg\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\">\n          <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M3 5a2 2 0 012-2h3.28a1 1 0 01.948.684l1.498 4.493a1 1 0 01-.502 1.21l-2.257 1.13a11.042 11.042 0 005.516 5.516l1.13-2.257a1 1 0 011.21-.502l4.493 1.498a1 1 0 01.684.949V19a2 2 0 01-2 2h-1C9.716 21 3 14.284 3 6V5z\" />\n        </svg>\n      </div>\n\n      <h3 class=\"standalone-phone-modal-title\">Call Us</h3>\n\n      <div class=\"standalone-store-status\" id=\"standalone-store-status\">\n        <span class=\"standalone-status-icon\">✓</span>\n        <span class=\"status-text\">We are currently open</span>\n      </div>\n\n      <div class=\"standalone-closed-message\" id=\"standalone-closed-message\" style=\"display: none;\">\n        <p>{{ closed_message }}</p>\n      </div>\n\n      <div class=\"standalone-opening-hours\">\n        <h4 class=\"standalone-hours-heading\">Opening Hours</h4>\n        <div class=\"standalone-hours-list\">\n          <div class=\"standalone-hours-row\">\n            <span class=\"day\">Monday</span>\n            <span class=\"time\">{{ monday_hours }}</span>\n          </div>\n          <div class=\"standalone-hours-row\">\n            <span class=\"day\">Tuesday</span>\n            <span class=\"time\">{{ tuesday_hours }}</span>\n          </div>\n          <div class=\"standalone-hours-row\">\n            <span class=\"day\">Wednesday</span>\n            <span class=\"time\">{{ wednesday_hours }}</span>\n          </div>\n          <div class=\"standalone-hours-row\">\n            <span class=\"day\">Thursday</span>\n            <span class=\"time\">{{ thursday_hours }}</span>\n          </div>\n          <div class=\"standalone-hours-row\">\n            <span class=\"day\">Friday</span>\n            <span class=\"time\">{{ friday_hours }}</span>\n          </div>\n          <div class=\"standalone-hours-row\">\n            <span class=\"day\">Saturday</span>\n            <span class=\"time\">{{ saturday_hours }}</span>\n          </div>\n          <div class=\"standalone-hours-row\">\n            <span class=\"day\">Sunday</span>\n            <span class=\"time\">{{ sunday_hours }}</span>\n          </div>\n        </div>\n      </div>\n\n      <a href=\"tel:{{ phone_number | replace: ' ', '' | replace: '-', '' }}\" class=\"standalone-call-now-button\" id=\"standalone-call-button\">\n        Call Now\n      </a>\n    </div>\n  </div>\n</div>\n\n<script>\n  function openStandaloneConsultationModal() {\n    const modal = document.getElementById('standalone-consultation-modal');\n    if (modal) {\n      modal.classList.add('active');\n      document.body.style.overflow = 'hidden';\n    }\n  }\n\n  function closeStandaloneConsultationModal() {\n    const modal = document.getElementById('standalone-consultation-modal');\n    if (modal) {\n      modal.classList.remove('active');\n      document.body.style.overflow = '';\n    }\n  }\n\n  function openStandalonePhoneHoursModal() {\n    closeStandaloneConsultationModal();\n    \n    const modal = document.getElementById('standalone-phone-hours-modal');\n    if (modal) {\n      modal.classList.add('active');\n      document.body.style.overflow = 'hidden';\n      checkStandaloneStoreStatus();\n      highlightStandaloneCurrentDay();\n    }\n  }\n\n  function closeStandalonePhoneHoursModal() {\n    const modal = document.getElementById('standalone-phone-hours-modal');\n    if (modal) {\n      modal.classList.remove('active');\n      document.body.style.overflow = '';\n    }\n  }\n\n  function checkStandaloneStoreStatus() {\n    const statusElement = document.getElementById('standalone-store-status');\n    if (!statusElement) return;\n\n    const now = new Date();\n    const currentDay = now.getDay();\n    const currentHour = now.getHours();\n    const currentMinute = now.getMinutes();\n    const currentTime = currentHour * 60 + currentMinute;\n\n    const modal = document.getElementById('standalone-phone-hours-modal');\n    const hoursRows = modal.querySelectorAll('.standalone-hours-row');\n    const todayRow = hoursRows[currentDay === 0 ? 6 : currentDay - 1];\n    const hoursText = todayRow.querySelector('.time').textContent.trim();\n\n    let isOpen = false;\n    if (hoursText.toLowerCase() !== 'closed') {\n      const matches = hoursText.match(/(\\d{1,2}):(\\d{2})(AM|PM)-(\\d{1,2}):(\\d{2})(AM|PM)/i);\n      if (matches) {\n        let openHour = parseInt(matches[1]);\n        const openMinute = parseInt(matches[2]);\n        const openPeriod = matches[3].toUpperCase();\n        let closeHour = parseInt(matches[4]);\n        const closeMinute = parseInt(matches[5]);\n        const closePeriod = matches[6].toUpperCase();\n\n        if (openPeriod === 'PM' && openHour !== 12) openHour += 12;\n        if (openPeriod === 'AM' && openHour === 12) openHour = 0;\n        if (closePeriod === 'PM' && closeHour !== 12) closeHour += 12;\n        if (closePeriod === 'AM' && closeHour === 12) closeHour = 0;\n\n        const openTime = openHour * 60 + openMinute;\n        const closeTime = closeHour * 60 + closeMinute;\n\n        isOpen = currentTime >= openTime && currentTime < closeTime;\n      }\n    }\n\n    const statusIcon = statusElement.querySelector('.standalone-status-icon');\n    const statusText = statusElement.querySelector('.status-text');\n    const closedMessage = document.getElementById('standalone-closed-message');\n    const callButton = document.getElementById('standalone-call-button');\n    \n    if (isOpen) {\n      statusElement.classList.remove('closed');\n      statusIcon.textContent = '✓';\n      statusText.textContent = 'We are currently open';\n      if (closedMessage) closedMessage.style.display = 'none';\n      if (callButton) {\n        callButton.textContent = 'Call Now';\n        callButton.classList.remove('disabled');\n        callButton.style.pointerEvents = 'auto';\n        callButton.style.background = '#007AFF';\n      }\n    } else {\n      statusElement.classList.add('closed');\n      statusIcon.textContent = '✗';\n      statusText.textContent = 'We are currently closed';\n      if (closedMessage) closedMessage.style.display = 'block';\n      if (callButton) {\n        callButton.textContent = 'Currently Closed';\n        callButton.classList.add('disabled');\n        callButton.style.pointerEvents = 'none';\n        callButton.style.background = '#cccccc';\n      }\n    }\n  }\n\n  function highlightStandaloneCurrentDay() {\n    const modal = document.getElementById('standalone-phone-hours-modal');\n    if (!modal) return;\n\n    const now = new Date();\n    const currentDay = now.getDay();\n    \n    const hoursRows = modal.querySelectorAll('.standalone-hours-row');\n    hoursRows.forEach(row => row.classList.remove('current-day'));\n    \n    const todayRow = hoursRows[currentDay === 0 ? 6 : currentDay - 1];\n    if (todayRow) {\n      todayRow.classList.add('current-day');\n    }\n  }\n\n  // Close modals when clicking on overlay\n  document.addEventListener('DOMContentLoaded', function() {\n    document.querySelectorAll('.standalone-consultation-modal-overlay').forEach(overlay => {\n      overlay.addEventListener('click', function(e) {\n        if (e.target === this) {\n          this.classList.remove('active');\n          document.body.style.overflow = '';\n        }\n      });\n    });\n\n    document.addEventListener('keydown', function(e) {\n      if (e.key === 'Escape') {\n        closeStandaloneConsultationModal();\n        closeStandalonePhoneHoursModal();\n      }\n    });\n  });\n</script>"
          }
