    "ss_simple_faq_jiYFWj": {
      "type": "ss-simple-faq",
      "blocks": {
        "text_Dpx4Qh": {
          "type": "text",
          "settings": {
            "question": "What is your cancellation policy?",
            "answer": "<p>Cancellations upto 24 hours before your appointment will receive a full refund or can be rescheduled.<br/><br/>Any cancellations on the day can be rescheduled but will not be refunded.</p>"
          }
        },
        "text_r9pPVi": {
          "type": "text",
          "settings": {
            "question": "I would like to speak to someone before I book my treatment, how is best to do this?",
            "answer": "<p>If you would like to speak to someone please do one of the below.<br/><br/>- Use the enquiry button.<br/>- Contact us via email at info@laserskinclinics.co.uk<br/>- Call us during opening hours on 01902 256667<br/>- Send us a whatsapp by clicking the Whatsapp button on the website.</p>"
          }
        },
        "text_3Bf6HW": {
          "type": "text",
          "settings": {
            "question": "When will I see results?",
            "answer": "<p>You’ll notice an immediate improvement in clarity, smoothness, and glow.<br/><br/>Pore size reduction and improved tone become more visible after a few sessions.</p>"
          }
        },
        "text_gRixBx": {
          "type": "text",
          "settings": {
            "question": "Who is the Carbon Facial suitable for?",
            "answer": "<p>This treatment is suitable for all skin types and is particularly beneficial for those with oily or acne-prone skin, enlarged pores, uneven tone, or dullness.<br/><br/>It is also safe for sensitive skin when performed by trained professionals.</p>"
          }
        },
        "text_JMUbWR": {
          "type": "text",
          "settings": {
            "question": "Does the treatment hurt?",
            "answer": "<p>No, the Carbon Facial is very comfortable.</p><p>You may feel a mild warmth or light snapping sensation as the laser works, but most clients find the experience relaxing.</p>"
          }
        },
        "text_D4FYRJ": {
          "type": "text",
