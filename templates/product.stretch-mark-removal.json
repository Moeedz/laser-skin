    "faq_updated_HwHp3d": {
      "type": "faq-updated",
      "blocks": {
        "faq_item_rryRKx": {
          "type": "faq_item",
          "settings": {
            "question": "What is stretch mark and scar removal?",
            "answer": "<p>Stretch mark and scar removal is a <strong>bespoke, targeted treatment programme</strong> designed to improve the appearance of stretch marks and scars by stimulating skin repair, collagen production, and tissue regeneration. Every plan is tailored to the <strong>type, depth, colour, and age</strong> of the mark or scar.</p>"
          }
        },
        "faq_item_ADtrJM": {
          "type": "faq_item",
          "settings": {
            "question": "What type of stretch marks and scars can be treated?",
            "answer": "<p>Treatment can help improve:</p><p></p><ul><li>Stretch marks (red, purple, or white)<br/></li><li>Acne scars<br/></li><li>Surgical scars<br/></li><li>Injury or trauma scars<br/></li><li>Textured or uneven skin<br/></li></ul><p></p>"
          }
        },
        "faq_item_KBVxnw": {
          "type": "faq_item",
          "settings": {
            "question": "How does the treatment work?",
            "answer": "<p>Treatment methods may include a combination of:</p><p></p><ul><li>Microneedling or RF microneedling<br/></li><li>C02 Laser Resurfacing<br/></li><li>Targeted skin therapies<br/></li><li>Regenerative techniques to boost collagen and elastin<br/></li></ul><p></p>"
          }
        },
        "faq_item_QFi9LT": {
          "type": "faq_item",
          "settings": {
            "question": "Is this a one size fits all treatment?",
            "answer": "<p>No.</p><p>We do <strong>not</strong> use a one-size-fits-all approach. Stretch marks and scars vary greatly, so each programme is fully customised following an in-depth consultation.</p>"
          }
        },
        "faq_item_e9fkc6": {
          "type": "faq_item",
          "settings": {
            "question": "Can I spread the cost?",
            "answer": "<p>Yes, we offer interest-free payment plans through Klarna and ShopPay.</p>"
          }
        }
      },
