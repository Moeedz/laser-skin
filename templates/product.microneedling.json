    "faq_updated_naJWxF": {
      "type": "faq-updated",
      "blocks": {
        "faq_item_fLCEtm": {
          "type": "faq_item",
          "settings": {
            "question": "How is RF Micro different from regular microneedling?",
            "answer": "<p>Traditional microneedling works mainly on the surface.</p><p>RF Micro delivers <strong>heat energy deep into the dermis</strong>, stimulating stronger collagen and elastin production, leading to <strong>firmer, longer-lasting results</strong>.</p>"
          }
        },
        "faq_item_3DFpRW": {
          "type": "faq_item",
          "settings": {
            "question": "Does the treatment hurt?",
            "answer": "<p>Most clients describe it as <strong>uncomfortable rather than painful</strong>.</p><p>A strong numbing cream is applied beforehand, and sensations are usually felt as heat or pressure rather than sharp pain.</p>"
          }
        },
        "faq_item_NqGWCf": {
          "type": "faq_item",
          "settings": {
            "question": "How long does a session last?",
            "answer": "<p>A typical session lasts <strong>45–60 minutes</strong>, </p>"
          }
        },
        "faq_item_zLy9i4": {
          "type": "faq_item",
          "settings": {
            "question": "Do you offer interest free finance?",
            "answer": "<p>Yes, we offer interest-free payment plans through Klarna and ShopPay</p>"
          }
        },
        "faq_item_fLPNMj": {
          "type": "faq_item",
          "settings": {
            "question": "Is the treatment safe for all skin types?",
            "answer": "<p>Yes. RF Micro is considered safe for <strong>all skin tones</strong>, including darker skin, when performed by trained professionals.</p>"
          }
        }
      },
      "block_order": [
        "faq_item_fLCEtm",
